---
import MainLayout from '@layouts/MainLayout.astro';
import Container from '@components/Container.astro';

export function getStaticPaths() {
  return [
    {params: {slug: 'autonav'}}
  ];
}

const { slug } = Astro.params;
---

<MainLayout>
    <Container>
        <p class="mb-2 font-semibold tracking block text-6xl lg:text-8xl leading-none tracking-tight">{slug}</span>
        </p>
    </Container>
</MainLayout>